{
  "root": true, // Don't traverse ancestor directories looking for .eslintrc[.*] files to merge.
  "presets": [ 
    "react",
    "es2016-stage1"
  ],
  "env": {
    "amd": true,
    "applescript": true,
    "browser": true,
    "commonjs": true,
    "embertest": true,
    "es6": true,
    "jasmine": true,
    "jest": true,
    "jquery": true,
    "meteor": true,
    "mocha": true,
    "mongo": true,
    "nashorn": true,
    "node": true,
    "phantomjs": true,
    "prototypejs": true,
    "protractor": true,
    "qunit": true,
    "serviceworker": true,
    "shelljs": true,
    "webextensions": true,
    "worker": true 
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },  
  "rules": {
    "curly": 0,
    "comma-dangle": [2, "always-multiline"],
    "comma-spacing": 0,
    "eqeqeq": [2, "allow-null"],
    "key-spacing": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "no-shadow": 0,
    "no-shadow-restricted-names": 0,
    "no-extend-native": 0,
    "no-var": 2,
    "new-cap": 0,
    "quotes": 0,
    "semi-spacing": 0,
    "space-unary-ops": 0,
    "space-infix-ops": 0,
    "consistent-return": 0,
    "strict": 0
  },
  "plugins": [
  ]
}
